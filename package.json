{
  "name": "dpannvelo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone X'",
    "test": "jest",
    "ipad": "react-native run-ios --device='iPad de Adrien'",
    "clear-cache-ios": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro* && rm -rf $TMPDIR/haste-*",
    "release-ios-test": "react-native run-ios  --configuration Release  --device='iPad de Adrien'",
    "release-iphone-test": "react-native run-ios  --configuration Release  --device='Jerem'",
    "iphone": "react-native run-ios --device='Testphone'",
    "iphone-lea": "react-native run-ios --device='Vends internet 1â‚¬ la minute'",
    "release-iphone-lea": "react-native run-ios --configuration Release --device='Nom'",
    "clean-release-android": "./gradlew clean app-ar-release.apk",
    "release-android-test": "react-native run-android  --variant=arRelease",
    "debug-android": "adb logcat *:E",
    "deploy-ios": "./build.ios.sh && yarn run release-ios-test",
    "deploy-android": "./build.android.sh && yarn run release-android-test",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle:ios": "react-native bundle   --entry-file   index.js  --bundle-output ios/main.jsbundle   --dev  false  --platform ios  --assets-dest  ios",
    "bundled-ios": "npm run bundle:ios",
    "release-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "standard": "cd src/ && standard --fix"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-firebase/app": "^12.7.1",
    "@react-native-firebase/auth": "^12.7.1",
    "@react-native-firebase/database": "^12.9.0",
    "@react-native-firebase/firestore": "^12.7.2",
    "@react-native-firebase/storage": "^12.7.4",
    "@react-native-picker/picker": "^2.1.0",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.0.6",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-document-picker": "^3.5.1",
    "react-native-elements": "^3.4.2",
    "react-native-fs": "^2.18.0",
    "react-native-geocoding": "^0.5.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-picker": "^4.0.6",
    "react-native-map-link": "^2.8.1",
    "react-native-maps": "0.28.0",
    "react-native-maps-directions": "^1.8.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^3.5.0",
    "react-native-toast-message": "^1.4.9",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
